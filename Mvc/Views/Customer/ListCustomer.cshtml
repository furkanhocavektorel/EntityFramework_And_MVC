@{
    Layout = null;
}
@using Mvc.Models.vmodel
@model CustomerListResponse

<html>

    <head>
    <meta charset="utf-8">

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">

    </head>

    <body>

    <div class="myheader " style="background-color: rgba(165, 180, 219, 0.39);">
        <header class="d-flex justify-content-center py-3">
            <ul class="nav nav-pills">
                <li class="nav-item"><a href="/home/index" class="nav-link " aria-current="page">Product</a></li>
                <li class="nav-item"><a href="/customer/listcustomer" class="nav-link active">Customers</a></li>
                <li class="nav-item"><a href="#" class="nav-link">Pricing</a></li>
                <li class="nav-item"><a href="#" class="nav-link">FAQs</a></li>
                <li class="nav-item"><a href="#" class="nav-link">About</a></li>
            </ul>
        </header>
    </div>


    <div class="mytable">
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th scope="col">AD</th>
                    <th scope="col">ŞİRKET ADİ</th>
                    <th scope="col">TEL</th>
                    <th scope="col">ÜLKE</th>
                    <th scope="col">ŞEHİR</th>
                    <th scope="col">ADRES</th>

                </tr>
            </thead>
            <tbody>
                @foreach(var item in Model.list){
                    <tr>
                        <td scope="row">@item.ContactName</td>
                        <td scope="row">@item.CompanyName</td>
                        <td scope="row">@item.Phone</td>
                        <td scope="row">@item.Country</td>
                        <td scope ="row">@item.City</td>
                        <td scope="row">@item.Address</td>
                    </tr>
                }
               
               
            
            </tbody>
        </table>



    </div>









    <script src="https://cdn.jsdelivr.net/npm/@@popperjs/core@2.11.8/dist/umd/popper.min.js" integrity="sha384-I7E8VVD/ismYTF4hNIPjVp/Zjvgyol6VFvRkX/vR+Vc4jQkC+hVqc2pM8ODewa9r" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.min.js" integrity="sha384-0pUGZvbkm6XF6gxjEnlmuGrJXVbNuzT9qBBavbLwCsOGabYfZo0T0to5eqruptLy" crossorigin="anonymous"></script>
    </body>


</html>